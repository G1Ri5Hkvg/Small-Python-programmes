factorial=int(input("Enter a number: "))
if factorial==0:
  print(1)
elif factorial>0:
  for i in range(1,factorial):
    factorial=factorial*i
  print(factorial)
elif factorial<0:
  factorial=abs(factorial)
  for i in range(1,factorial):
    factorial=factorial*i
  print(-factorial)
else:
  print("Enter an integer")
